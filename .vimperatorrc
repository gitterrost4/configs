command -nargs=1 bug open http://bugzilla.hal.dbrent.net/buglist.cgi?quicksearch=<args>
noremap ,b :bug 
command -nargs=1 tabbug tabopen http://bugzilla.hal.dbrent.net/buglist.cgi?quicksearch=<args>
noremap ,B :tabbug 
command -nargs=1 dbdoc open http://doc.hal.dbrent.net/hal2dbdocs/dbcarsharing/table.<args>.html
noremap ,d :dbdoc 
command -nargs=1 tabdbdoc tabopen http://doc.hal.dbrent.net/hal2dbdocs/dbcarsharing/table.<args>.html
noremap ,D :tabdbdoc 
command -nargs=1 dbdoclong open http://doc.hal.dbrent.net/hal2dbdocs/dbcarsharing_long/table.<args>.html
noremap ,l :dbdoclong 
command -nargs=1 tabdbdoclong tabopen http://doc.hal.dbrent.net/hal2dbdocs/dbcarsharing_long/table.<args>.html
noremap ,L :tabdbdoclong 
command -nargs=0 branch open http://branch.virt.hal2/statistik/nstat.php
command -nargs=0 tbranch tabopen http://branch.virt.hal2/statistik/nstat.php
command -nargs=0 dev open http://dev.virt.hal2/statistik/nstat.php
command -nargs=0 tdev tabopen http://dev.virt.hal2/statistik/nstat.php
command -nargs=0 live open http://live.virt.hal2/statistik/nstat.php
command -nargs=0 tlive tabopen http://live.virt.hal2/statistik/nstat.php
map ,,B :tabopen http://bugzilla.hal.dbrent.net/buglist.cgi?cmdtype=runnamed&namedcmd=Really_My_Bugs<CR>
map ,,b :open http://bugzilla.hal.dbrent.net/buglist.cgi?cmdtype=runnamed&namedcmd=Really_My_Bugs<CR>
command atoss open http://ases.noncd.rz.db.de/SES/client/sesWebstart.jnlp
com -nargs=1 projecteuler open http://projecteuler.net/index.php?section=problems&id=<args>
set noautocomplete
set toolbars=noaddons,nobookmarks,nomenu,nonavigation,tabs
set hintchars=aoeuhtns
map ,r oreddit.com<CR>
highlight Hint font-size:100%;color:black;background:rgba(255,255,0,1);padding:2px;border:1px solid black;text-transform:uppercase;
set fc
set nosb
highlight HintElem background:rgba(255,200,0,0.5);
map <F10> :set<Space>toolbars=tabs<CR>
map <F9> :set<Space>toolbars=notabs<CR>
set toolbars=notabs

